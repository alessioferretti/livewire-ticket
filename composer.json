{
    "name": "alessio-ferretti/livewire-ticket",
    "description": ":Mini issue ticket panel in livewire",
    "license": "MIT",
    "authors": [
        {
            "name": "Alessio Ferretti"

        }
    ],
    "homepage": "https://github.com/alessioferretti/livewire-ticket",
    "keywords": ["Laravel", "LivewireTicket"],
    "require": {
        "php": ">=8.1",
        "livewire/livewire": "^2.4",
        "rappasoft/laravel-livewire-tables": "^2.15"

    },
    "require-dev": {

    },
    "autoload": {
        "psr-4": {
            "AlessioFerretti\\LivewireTicket\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlessioFerretti\\LivewireTicket\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlessioFerretti\\LivewireTicket\\LivewireTicketServiceProvider"
            ],
            "aliases": {
                "LivewireTicket": "AlessioFerretti\\LivewireTicket\\Facades\\LivewireTicket"
            }
        }
    }
}
